#include <iostream>
#include "MatrixKlassen/matrix.hh"
using namespace std;

int main() {

	Matrix<double> M(1);

	M(1,1) = 5.6;

	std::vector<double> v(1), result(1);

	v[0]=1;

	M(v, result);

	return 0;
}

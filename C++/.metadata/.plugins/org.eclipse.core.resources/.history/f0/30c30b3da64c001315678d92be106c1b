int n=50,N=n*n;
Matrix<double> A(N)	;
A=Erstelle(n, n);

Vector g(N), f(N),b(N),o(N),erg(N);
f=F(n);
// cout << f << endl;
g=G(n);
b=B(n);
o=f.operator+(b);
erg=CG(A,o);
